{
    "exp_name": "resnet",
    "mode": "train",
    "split":{
        "train": ["00009", "00012", "00005", "00020", "00031", "00022", "00029", "00016", "00026", "00024", "00014", "00010", "00011", "00027", "00008", "00019", "00030", "00003", "00025", "00006", "00033"], 
        "valid": ["00013", "00018", "00002", "00032", "00028", "00021", "00000"], 
        "test": ["00023", "00004", "00040", "00015", "00017", "00001", "00007"], 
        "5-Fold": {"Fold-1": ["00023", "00004", "00040", "00015", "00017", "00001", "00007"], 
            "Fold-2": ["00013", "00018", "00002", "00032", "00028", "00021", "00000"], 
            "Fold-3": ["00009", "00012", "00005", "00020", "00031", "00022", "00029"],
            "Fold-4": ["00016", "00026", "00024", "00014", "00010", "00011", "00027"], 
            "Fold-5": ["00008", "00019", "00030", "00003", "00025", "00006", "00033"]
    }
    },
    "dataset":{
        "ring_type": "ring2",
        "input_type": ["ir-filtered", "red-filtered","ax-filtered","ay-filtered","az-filtered"],
        "label_type": ["samsung_hr","oura_hr","hr", "spo2", "bvp_sdnn","resp_rr","BP_sys","BP_dia"],
        "shuffle": true,
        "batch_size": 32,
        "quality_assessment": {
            "method": "elgendi",
            "th": 0.8
        },
        "target_fs": 100,
        "window_duration": 30,
        "experiment": ["Health", "Daily","Sport"],
        "task": ["sitting", "spo2", "deepsquat", "talking", "shaking_head", "standing", "striding"]
    },
    "seed": 42,
    "csv_path": "csv/resnet/resnet.csv",
    "img_path": "img/resnet",
    "method": {
        "name": "resnet",
        "type": "ML", 
        "model_path": null,
        "params":{
            "in_channels": 5,  
            "base_filters": 32,
            "kernel_size": 5,
            "stride": 1,
            "groups": 1,
            "n_block": 8,
            "downsample_gap": 2,
            "increasefilter_gap": 2,
            "backbone": false
        }
    },
    "train":{
        "device": "0",
        "epochs": 300,
        "lr": 5e-4,
        "criterion": "mse",
        "optimizer": "adamw",
        "early_stopping": {
            "monitor": "val_loss",
            "patience": 40,
            "mode": "min"
        },
        "scheduler": {
            "type": "reduce_on_plateau",
            "factor": 0.5,
            "patience": 10,
            "threshold": 1e-4,
            "min_lr": 1e-6
        },
        "model_checkpoint": {
            "monitor": "val_loss",
            "mode": "min",
            "save_best_only": true
        }
    },
    "test":{
        "device": "0",
        "batch_size": 32,
        "metrics": ["mae", "rmse", "mape", "pearson"],
        "model_path": null,
        "model_name": null
    },
    "pretrain_model": "TODO"
}